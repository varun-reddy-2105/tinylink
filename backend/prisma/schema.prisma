generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Link {
  id            Int      @id @default(autoincrement())
  code          String   @unique
  targetUrl     String
  totalClicks   Int      @default(0)
  createdAt     DateTime @default(now())
  lastClickedAt DateTime?
}
